{
  "name": "libreformbuilder",
  "version": "0.0.1",
  "description": "React frontend for WP Libre Formbuilder",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/react-app-rewired/bin/index.js start",
    "build": "node ./node_modules/react-app-rewired/bin/index.js build",
    "test": "react-app-rewrired --env=jsdom",
    "eject": "react-app-rewrired eject"
  },
  "author": "Christian Nikkanen",
  "license": "GPL-2.0",
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^~": "<rootDir>/app",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/style-mock.js"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "es6-promise": "^4.2.4",
    "history": "^4.7.2",
    "html-react-parser": "^0.4.6",
    "kea": "^0.27.8",
    "kea-localstorage": "^0.2.0",
    "kea-saga": "^0.3.2",
    "lodash": "^4.17.10",
    "nprogress": "^0.2.0",
    "pretty": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.2.0",
    "react-modal": "^3.4.4",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "node-sass": "^4.9.1",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewire-scss": "^1.0.2",
    "react-app-rewired": "^1.5.2",
    "redux-devtools": "^3.4.1",
    "sass-loader": "^7.0.3"
  }
}
